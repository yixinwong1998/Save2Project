{
  "manifest_version": 3,
  "name": "Save2Project",
  "version": "0.0.1",
  "icons": {
    "512": "./icon.png"
  },
  "description": "Save something to the on-going project directory locally as a resource file.",
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "downloads", "storage"],
  "web_accessible_resources": [
    {
      "resources": ["welcome.html"],
      "matches": ["<all_urls>"]
    }
  ]
}